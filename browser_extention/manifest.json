{
  "manifest_version": 2,
  "name": "Random Youtube Mix",
  "version": "0.1",
    "browser_action": {
      "default_icon": "icons/icon.png" 
    },
    "icons": {
        "16": "icons/icon.png",
        "48": "icons/icon.png",
        "128": "icons/icon.png"
    },
  "description":"Open a random saved youtube mix",
    "background" : {
      "scripts" : ["background.js"]
    },
    "permissions":["tabs","https://www.youtube.com/"]
}